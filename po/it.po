msgstr "Interagisce con i repository Copr."

#: ../plugins/copr.py:59
msgid ""
"\n"
"  enable name/project [chroot]\n"
"  disable name/project\n"
"  remove name/project\n"
"  list name\n"
"  search project\n"
"\n"
"  Examples:\n"
"  copr enable rhscl/perl516 epel-6-x86_64\n"
"  copr enable ignatenkobrain/ocltoys\n"
"  copr disable rhscl/perl516\n"
"  copr remove rhscl/perl516\n"
"  copr list ignatenkobrain\n"
"  copr search tests\n"
"    "
msgstr ""
"\n"
"  enable nome/progetto [chroot]\n"
"  disable nome/progetto\n"
"  remove nome/progetto\n"
"  list nome\n"
"  search progetto\n"
"\n"
"  Esempi:\n"
"  copr enable rhscl/perl516 epel-6-x86_64\n"
"  copr enable ignatenkobrain/ocltoys\n"
"  copr disable rhscl/perl516\n"
"  copr remove rhscl/perl516\n"
"  copr list ignatenkobrain\n"
"  copr search tests\n"
"    "

